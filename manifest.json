{
  "manifest_version": 2,
  "name": "Beautiful Github",
  "description": "Customize github color based on User selection",
  "version": "1.0",
  "icons": {
    "48": "icons/logo_48.png",
    "64": "icons/logo_64.png"
  },
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "icons/logo_64.png",
    "default_title": "Beatiful Github",
    "default_popup": "index.html"
  }
}
